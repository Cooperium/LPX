@model L.PX.Models.LogOnModel
@{
    ViewBag.Title = "TelaAddParticipante";
}

<h2>Adicionar Participante</h2>


<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>



@using (Html.BeginForm()) {
    
        <fieldset>
        <legend>Participante</legend>
        <p>Entre com o Email do novo participante do leilão </p>
        <div class="editor-label">
                @Html.LabelFor(m => m.Email)
            </div>
           <div class="editor-field">
                 @Html.DropDownList("Participantes", String.Empty)
                 @Html.ValidationMessageFor(model => model.Email )
        </div>
        </fieldset>
    <input type="submit" value="Mais" /> 
    <input type="submit" value="Enviar" />
    
}
