@model L.PX.Core.Leilao
@{
    ViewBag.Title = "TelaGerenciadorPart";
}
<h2>
    TelaGerenciadorPart</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{

    <h3>
        Participante</h3>
    <fieldset>
        <legend>Dados do Leilão</legend>
        <div class="display-label">
            Identificador</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Id)</div>
        <div class="display-label">
            Valor inicial</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.ValorInicial)</div>
        <div class="display-label">
            Número de lotes</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.NumeroDeLotes)
            <br />
    </fieldset>
    <br />
   
    <input type="submit" value="Voltar" />
    
}
