@model L.PX.Core.LanceProcessado
@{
    ViewBag.Title = "TelaContratante";
 }
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    
    <h3>
        Contratante</h3>
    <fieldset>
        <legend>Seu lance:</legend>
                @Html.EditorForModel()
    </fieldset>
    <fieldset>
        <legend>Incremento</legend>
        <p>
            </p>
        <div class="editor-label">
            @Html.Label("Lance Inicial")
        </div>                    
        <div class="editor-field">
            @Html.TextBox("Lance Inicial")
        </div>
        <div class="editor-label">
            @Html.Label("Incremento")]
        </div>
        <div class="editor-field">
            @Html.TextBox("Incremento")
         </div>
    </fieldset>
    
    <input type="submit" value="OK" />
}
